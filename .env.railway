# Railway Production Environment Variables
# These should be set in Railway's dashboard, not committed to git

NODE_ENV=production
PORT=$PORT

# Database - Railway will provide PostgreSQL
DATABASE_URL=$DATABASE_URL

# Logging
LOG_LEVEL=info

# Security - Update with your actual domain
CORS_ORIGIN=https://your-app-name.up.railway.app

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000

# Redis - Railway Redis addon
USE_REDIS=true
REDIS_URL=$REDIS_URL
REDIS_KEY_PREFIX=identity-api: